<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="480px"
    android:layout_height="800px"
    android:background="#f6f6f6"
    android:orientation="vertical" >

    <LinearLayout
        android:gravity="center"
        android:background="@color/blue_title"
        android:layout_width="match_parent"
        android:layout_height="123px" >

        <ImageView
            android:layout_marginRight="15px"
            android:id="@+id/imageView1"
            android:layout_width="49px"
            android:layout_height="50px"
            android:src="@drawable/momey_img" />

        <TextView
            style="@style/px_30"
            android:textColor="@color/white"
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清机加钞信息" />

        <ImageView
            android:layout_marginLeft="145px"
            android:id="@+id/clearresult_back"
            android:layout_width="64px"
            android:layout_height="65px"
            android:src="@drawable/back_cirle" />

    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_marginLeft="20px"
                android:gravity="center"
                android:textColor="@color/red"
                android:id="@+id/TextView03"
                style="@style/px_25"
                android:layout_width="wrap_content"
                android:layout_height="40px"
                android:text="所在位置:"
                 />

            <TextView
                android:id="@+id/clear_result_address"
                style="@style/px_25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/back" />
        </TableRow>

        <TableRow
            android:id="@+id/TableRow01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_marginLeft="20px"
                android:gravity="center"
                android:id="@+id/TextView01"
                style="@style/px_25"
                android:layout_width="wrap_content"
                android:layout_height="40px"
                android:text="机构编号:"
                android:textColor="@color/back" />

            <TextView
                android:id="@+id/clear_restlu_corpid"
                style="@style/px_25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/back" />
        </TableRow>

        <TableRow            
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:layout_marginLeft="20px"
                style="@style/px_25"
                android:textColor="@color/back"
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="40px"
                android:text="柜员编号:" />

            <TextView
                style="@style/px_25"
                android:textColor="@color/back"
                android:id="@+id/clear_result_moneyNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="" />

        </TableRow>
    </TableLayout>

    <LinearLayout
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TableLayout
            android:layout_marginRight="5px"
            android:layout_marginLeft="5px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TableRow
                android:id="@+id/TableRow03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/gray" >

                <TextView
                    android:id="@+id/TextView06"
                    style="@style/px_25"
                    android:layout_width="150px"
                    android:layout_height="60px"
                    android:layout_margin="1px"
                    android:background="@color/white"
                    android:gravity="center"
                    android:text="ATM编号"
                    android:textColor="@color/back" />

                <TextView
                    android:id="@+id/TextView05"
                    style="@style/px_25"
                    android:layout_width="150px"
                    android:layout_height="60px"
                    android:layout_marginBottom="1px"
                    android:layout_marginTop="1px"
                    android:background="@color/white"
                    android:gravity="center"
                    android:text="钞箱编号"
                    android:textColor="@color/back" />

                <TextView
                    android:id="@+id/TextView07"
                    style="@style/px_25"
                    android:layout_width="150px"
                    android:layout_height="60px"
                    android:layout_margin="1px"
                    android:background="@color/white"
                    android:gravity="center"
                    android:text="结果"
                    android:textColor="@color/back" />
            </TableRow>

            <TableRow
                android:background="@color/gray"
                android:id="@+id/tableRow6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

            </TableRow>
        </TableLayout>

        <ListView
            android:dividerHeight="1px"
            android:layout_marginRight="14px"
            android:layout_marginLeft="15px"
            android:id="@+id/listview_clearresult"
            android:layout_width="455px"
            android:layout_height="310px" >
        </ListView>

        <LinearLayout
            android:layout_marginTop="30px"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="70px" >

            <Button
                style="@style/px_30"
                android:textColor="@color/white"
                android:background="@drawable/buttom_selector_bg"
                android:id="@+id/sure_clearresult"
                android:layout_width="300px"
                android:layout_height="match_parent"
                android:text="确定" />

        </LinearLayout>

    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>
    </TableLayout>

</LinearLayout>